{
  "departmentId": "12345",
  "reporter": {
    "userConsent": true,
    "trigger": {
      "schedule": "*/1 * * * *"
    }
  },
  "destinations": {
    "incidents": {
      "s3": {
        "Bucket": "my-bucket",
        "folder": "my-folder",
        "accessKeyId": "AccessID",
        "secretAccessKey": "secretAccess"
      }
    }
  },
  "sources": {
    "testFiles": {
      "enabled": true,
      "destination": "incidents",
      "fileWatch": {
        "folder": "./test/data/testFolder/",
        "processed": {
            "folder": "./processed/"
        }
      }
    },
    "networkFiles": {
      "enabled": true,
      "destination": "incidents",
      "fileWatch": {
        "folder": "Z:\\",
        "processed": {
            "folder": "./processed/"
        }
      }
    }
  }
}
