#!/usr/bin/groovy

node('windows') {
  def root = pwd()

  stage('Setup') {
    git url: 'https://github.com/StatEngine/spade', branch: 'master'
  }

  stage('Archive') {
    withEnv(["ARTIFACT_BUCKET=\$ARTIFACT_BUCKET"]) {
      sh """
        ./clean-install.sh
        npm run package
      """
    }
  }
}
